#===============================================================================================================
# [[0200]] Global overriding rules
#===============================================================================================================

#!! Waypoint c0.alpha : "All Rules - Highest priority - DANGER ZONE"

Section 6Links : Auto("6L"), LinkedSockets 6, Rarity <= Rare
{
	Show hightier { !G2; !6linkhigh; Clean(); BaseType == bases.sixlink.valuable.t1; ItemLevel >= 75; }
	Show others   { %D5; !G3; !6linklow; Empty(); }
}

#===============================================================================================================
# [[0200]] Gold
#===============================================================================================================

Section Gold : Auto("Gold"), BaseType == "Gold"
{
	Show stack3 { !G64; !gold.huge; StackSize >= 3001; }
	Show stack2 { !G5;  !gold.large; StackSize >= 500; }
	Show stack1 { !G6H; !gold.medium; StackSize >= 150; }
	
	Show stacklvl1 { !G5; !gold.medium; StackSize >= 50; AreaLevel <= 68; }
	
	Show anyOther   { !G6H; !gold.small; Empty(); }
}

