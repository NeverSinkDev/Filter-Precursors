#------------------------------------
#   [1234] Common Tier - T1
#------------------------------------
#!! Waypoint c1.infl.all : "All rules - except 6links and influenced maps"

Section MemoryStrandEquip : Auto("gear->memorystrand"), Rarity <= Rare
{
	var combinedt1gear = source.rares.eg.t1 + source.crafting.general.t1;
	var combinedt2gear = source.rares.eg.t2 + source.crafting.general.t2 - combinedt1gear;
	var combinedt3gear = source.rares.eg.t3 + source.crafting.general.t3 - combinedt2gear;

	Show strandedFractured { !G2; !exotic.t1; MemoryStrands >= 1; FracturedItem True; }
	Show strandedVeiled { !G2; !exotic.t1; MemoryStrands >= 1; Identified True; Mod "Veil"; }

	Show t1high { !G2; !exotic.t1; MemoryStrands >= 60; BaseType == combinedt1gear; }
	Show t1 	{ !G2; !exotic.t1; MemoryStrands >= 1;  BaseType == combinedt1gear; }

	Show t2high { !G2; !exotic.t1; MemoryStrands >= 60; BaseType == combinedt2gear; }
	Show t2 	{ !G3; !exotic.t2; MemoryStrands >= 1;  BaseType == combinedt2gear; }

	Show t3high { !G2; !exotic.t1; MemoryStrands >= 60; BaseType == combinedt3gear; }
	Show t3 	{ !G3; !exotic.t2; MemoryStrands >= 1;  BaseType == combinedt3gear; }

	Show AnyHigh { !GSH; !exotic.t3; MemoryStrands >= 60; }
	Show Any 	 { %D5; !GSH; !exotic.t3; MemoryStrands >= 1; }
}

